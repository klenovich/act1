{
  "expo": {
    "name": "ChatGPT",
    "slug": "chatgpt-mobile",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#FFFFFF",
      "resizeMode": "contain",
      "dark": {
        "image": "./assets/splash_dark.png",
        "backgroundColor": "#343541",
        "resizeMode": "contain"
      }
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
